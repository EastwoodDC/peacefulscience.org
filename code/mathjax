echo MATHJAX:

if [ -f hugo.log ]
then
  echo "Reading MATHJAX files from 'hugo.log'."
else
  echo "Can't find 'hugo.log'."
  echo
  exit
fi

for file in `awk  '/MATHJAX/ {print $5}' hugo.log`
do
  i=public${file}index.html
  echo $i
  npx mjpage < $i > $i.new 2> /dev/null && mv $i.new $i &

  i=public/_prince${file}index.html
  echo $i
  npx mjpage < $i > $i.new 2> /dev/null && mv $i.new $i &

  wait
done

echo
